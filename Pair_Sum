from os import *
from sys import *
from collections import *
from math import *

def pairSum(arr, s):
    # Write your code here.
    a=[]
    arr.sort()
    l=len(arr)
    for i in range(l):
        for j in range(i+1,l):
            if(arr[i]+arr[j]==s):
                a.append([arr[i],arr[j]])            
    return a
    pass
